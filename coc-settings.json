{
  "coc.preferences.formatOnSave": false,
  "npm.binPath": "npm",
  "javascript.showUnused": false,
  "javascript.showDeprecated": false,
  "typescript.showUnused": false,
  "inlayHint.enable": false,
  "typescript.showDeprecated": false,
  "css.validate": false,
  "html-css-support.enabledLanguages": [
    "html",
    "vue",
    "javascriptreact",
    "typescriptreact",
    "angular"
  ],
  "emmet.includeLanguages": {
    "html": "html",
    "vue": "html",
    "vue-html": "html"
  },
  "emmet.excludeLanguages": [
    "markdown",
    "js",
    "ts"
  ],
  "pairs.enableCharacters": [
    "(",
    "[",
    "{",
    "'",
    "\"",
    "`"
  ],
  "vetur.ignoreProjectWarning": false,
  "vetur.validation.style": false,
  "vetur.validation.script": false,
  "vetur.format.styleInitialIndent": true,
  "vetur.format.scriptInitialIndent": true,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "vue",
    "jsx",
    "tsx"
  ],
  "tailwindCSS.includeLanguages": {
    "eelixir": "html",
    "elixir": "html",
    "eruby": "html",
    "html.twig": "html",
    "vue": "html",
    "css": "html",
    "vue-html": "html"
  },
  "clangd.trace.server": "messages",
  "prettier.vueIndentScriptAndStyle": true,
  "languageserver": {
    "lua": {
      "command": "lua-language-server",
      "filetypes": [
        "lua"
      ],
      "trace.server": "verbose",
      "rootPatterns": [
        ".git/",
        ".editorconfig"
      ],
      "settings": {
        "workspace": {
          "library": [],
          "maxPreload": 2000,
          "preloadFileSize": 1000
        },
        "runtime": {
          "version": "Lua 5.4"
        },
        "diagnostics": {
          "enable": true,
          "globals": [
            "hs",
            "vim",
            "it",
            "describe",
            "before_each",
            "after_each"
          ],
          "disable": [
            "lowercase-global"
          ]
        },
        "completion": {
          "keywordSnippet": "Disable"
        }
      }
    }
  }
}
