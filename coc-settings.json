{
    "coc.preferences.formatOnSave": false,
    "suggest.floatConfig": {
        "border": true,
        "rounded": true
    },
    "signature.floatConfig": {
        "rounded": true,
        "border": true
    },
    "hover.floatConfig": {
        "maxWidth": 50
    },
    "suggest.completionItemKindLabels": {
        "text": " text",
        "method": " method",
        "function": " function",
        "constructor": " constructor",
        "field": " field",
        "variable": " variable",
        "class": " class",
        "interface": " interface",
        "module": " module",
        "property": " property",
        "unit": " unit",
        "value": " value",
        "enum": " enum",
        "keyword": " keyword",
        "snippet": " snippet",
        "color": " color",
        "file": " file",
        "reference": " reference",
        "folder": " folder",
        "enumMember": " enum member",
        "constant": " constant",
        "struct": " struct",
        "event": " event",
        "operator": " operator",
        "typeParameter": " type parameter",
        "default": " default"
    },
    "npm.binPath": "npm",
    "javascript.showUnused": false,
    "javascript.showDeprecated": false,
    "typescript.showUnused": false,
    "inlayHint.enable": false,
    "typescript.showDeprecated": false,
    "css.validate": false,
    "html-css-support.enabledLanguages": [
        "html",
        "vue",
        "javascriptreact",
        "typescriptreact",
        "angular",
        "javascript"
    ],
    "emmet.includeLanguages": {
        "html": "html",
        "vue": "html",
        "vue-html": "html"
    },
    "emmet.excludeLanguages": [
        "markdown"
    ],
    "pairs.enableCharacters": [
        "(",
        "[",
        "{",
        "'",
        "\"",
        "`"
    ],
    "vetur.ignoreProjectWarning": false,
    "vetur.validation.style": false,
    "vetur.validation.script": false,
    "vetur.format.styleInitialIndent": true,
    "vetur.format.scriptInitialIndent": true,
    "html.filetypes": [
        "html",
        "handlebars",
        "htmldjango",
        "blade",
        "vue",
        "jsx",
        "tsx"
    ],
    "tailwindCSS.includeLanguages": {
        "eelixir": "html",
        "elixir": "html",
        "eruby": "html",
        "html.twig": "html",
        "vue": "html",
        "css": "html",
        "vue-html": "html"
    },
    "clangd.trace.server": "messages",
    "prettier.vueIndentScriptAndStyle": true,
    "zig.enabled": true,
    "zig.startUpMessage": true,
    "zig.path": "zls",
    "zig.debugLog": false,
    "languageserver": {
        "glsl": {
            "command": "$HOME/lang-server/glsl_analyzer",
            "filetypes": [
                "glsl"
            ],
            "trace.server": "verbose",
            "rootPatterns": [
                ".editorconfig"
            ]
        },
        "rust": {
            "command": "$HOME/lang-server/rust-ls",
            "filetypes": [
                "rs",
                "rust"
            ],
            "trace.server": "verbose",
            "rootPatterns": [
                ".editorconfig"
            ]
        },
        "zig": {
            "command": "$HOME/lang-server/zig-ls",
            "filetypes": [
                "zig"
            ],
            "trace.server": "verbose",
            "rootPatterns": [
                ".editorconfig"
            ]
        },
        "lua": {
            "command": "lua-language-server",
            "filetypes": [
                "lua"
            ],
            "trace.server": "off",
            "rootPatterns": [
                ".editorconfig",
                ".luarc.json",
                ".git/",
                "main.lua",
                "init.lua"
            ]
        }
    }
}
